create project
npm init vue@latest

cd <project>

npm install
npm run lint
npm run dev

npm install -D tailwindcss postcss autoprefixer
npx tailwindcss init -p

add to tailwind.config.js
content: [
    "./index.html",
    "./src/**/*.{js,ts,jsx,tsx}",
  ],

new src/index.css
@tailwind base;
@tailwind components;
@tailwind utilities;
